            class="d-flex justify-content-center"
            class="d-flex align-items-center"
          >
            <h4 style="margin-bottom: 20px;">{{client}}</h4>
            <h3 class="shadowText" style="margin-bottom: 165px;">{{note}}</h3>
          </div>
        </div>
      {{/each}}

